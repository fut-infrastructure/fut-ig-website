         fhir:v "https://measurement.devtest.systematic-ehealth.com/fhir/OperationDefinition/-s-meta"^^xsd:anyURI ;
         fhir:link <https://measurement.devtest.systematic-ehealth.com/fhir/OperationDefinition/-s-meta>
       ]
